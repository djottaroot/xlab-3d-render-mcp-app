{
  "name": "@djotta/3d-render-mcp",
  "version": "1.0.0",
  "type": "module",
  "description": "3D Architectural Render MCP App using Three.js and JSCAD",
  "license": "MIT",
  "main": "dist/server.js",
  "bin": {
    "3d-render-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc --noEmit && cross-env INPUT=src/mcp-app.html vite build && mv dist/src/mcp-app.html dist/mcp-app.html && rmdir dist/src && tsc -p tsconfig.server.json && bun build src/server.ts --outdir dist --target node && bun build src/main.ts --outfile dist/index.js --target node --banner \"#!/usr/bin/env node\"",
    "watch": "cross-env INPUT=src/mcp-app.html vite build --watch",
    "serve": "bun --watch src/main.ts",
    "start": "bun dist/index.js",
    "dev": "concurrently 'bun run watch' 'cross-env PORT=3000 bun --watch src/main.ts'",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@jscad/io": "2.4.13",
    "@jscad/modeling": "2.12.7",
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@upstash/redis": "^1.36.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^12.0.0",
    "gsap": "^3.14.2",
    "i18next": "^25.8.6",
    "i18next-browser-languagedetector": "^8.2.1",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.475.0",
    "mcp-handler": "^1.0.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^16.5.4",
    "three": "^0.173.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/three": "^0.173.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vite-plugin-singlefile": "^2.0.0"
  }
}